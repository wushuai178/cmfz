<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.ws.dao.ChatDao">
    <select id="queryByTime" resultType="com.baizhi.ws.entity.Chat">
        select id,content,create_date createDate,user_name userName from chat order by create_date asc
    </select>
</mapper>
